<h1>Edit Song</h1>

<%= form_for(@song) do |f| %>
    <p>
        <%= f.label :artist_name %>
        <%= f.text_field :artist_name %>
    </p>

    <p>
        <%= f.label :title %>
        <%= f.text_field :title %>
    </p>

    <p>
        <%= f.label :genre %>
        <%= f.text_field :genre %>
    </p>

    <p>
        <%= f.label :released %>
        <%= f.check_box :released %>
    </p>

    <p>
        <%= f.label :release_year %>
        <%= f.text_field :release_year %>
    </p>

    <%= f.submit %>
<% end %>

<br />

<%= link_to "Songs Index", songs_url %>