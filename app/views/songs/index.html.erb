<h1>All Songs</h1>

<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Artist</th>
            <th>Genre</th>
            <th>Released Year</th>
        </tr>
    </thead>
    <tbody>
        <% @songs.each do |s| %>

            <tr>
                <td><%= link_to s.title, song_path(s) %></td>
                <td><%= s.artist_name %></td>
                <td><%= s.genre %></td>
                <td><%= s.release_year %></td>
            </tr>
        <% end %>
    </tbody>
</table>