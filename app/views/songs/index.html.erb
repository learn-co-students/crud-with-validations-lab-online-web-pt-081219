<h1>All Songs</h1>
<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Artist Name</th>
            <th>Released</th>
            <th>Release Year</th>
            <th>Genre</th>
            <th>View</th>
            <th>Edit</th>
            <th>Delete</th>
        </tr>
    </thead>

    <tbody>
        <% @songs.each do |s| %>
        <tr>
            <td><%= s.title %></td>
            <td><%= s.artist_name %></td>
            <td><%= s.released %></td>
            <td><%= s.release_year %></td>
            <td><%= s.genre %></td>
            <td><%= link_to 'View Song', s %></td>
            <td><%= link_to 'Edit Song', edit_song_path(s) %></td>
            <td><%= link_to 'Delete Song', s, method: :delete, data: { confirm: "Are you sure?"} %></td>
        </tr>
        <% end %>
    </tbody>
</table>