<h1>Songs</h1>

<table>
  <thead>
    <tr>
      <th>Song Title</th>
    </tr>
  </thead>
  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= link_to song.title, song_path(song) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<p><%= link_to "Create New Song", new_song_path %></p>