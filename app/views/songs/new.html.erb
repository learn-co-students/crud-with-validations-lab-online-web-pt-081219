<h1>Create New Song</h1>

<%= form_for(@song) do |f| %>
  <div class="field">
    <%= f.label "Song Title" %> <br />
    <%= f.text_field :title %><br>
  </div>
  <div class="field">
      <%= f.label "Released" %> <br />
      <%= f.check_box :released, {}, true, false %> <br />
  </div>
  <div class="field">
    <%= f.label "Release Year" %> <br />
    <%= f.text_field :release_year %><br>
  </div>
  <div class="field">
    <%= f.label "Artist Name" %> <br />
    <%= f.text_field :artist_name %><br>
  </div>
  <div class="field">
    <%= f.label "Genre" %> <br />
    <%= f.text_field :genre %><br>
  </div>

  <%= f.submit %>
<% end %>